{
  "name": "express-test",
  "version": "1.0.0",
  "main": "src/server.js",
  "extension": "js",
  "license": "MIT",
  "type": "commonjs",
  "author": "github.com/kotstok",
  "private": true,
  "scripts": {
    "prestart": "babel ./src --out-dir dist",
    "start": "yarn prestart && node ./dist/server.js",
    "start:no-build": "node ./dist/server.js",
    "start:dev": "nodemon --exec babel-node ./src/server.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**'",
    "postpretty": "yarn lint --fix",
    "db:dev:up": "docker compose --env-file ./.env up dev-db -d",
    "db:dev:rm": "docker compose rm dev-db -s -f -v && docker container rm dev-db -f -v",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up",
    "db:test:up": "docker compose --env-file ./.env.test up test-db -d",
    "db:test:rm": "docker compose rm test-db -s -f -v && docker container rm test-db -f -v",
    "db:test:restart": "db:test:rm && yarn db:test:up"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "async-jsonwebtoken": "^1.0.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "winston": "^3.8.2",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.7",
    "babel-register": "^6.26.0",
    "eslint": ">=5.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "http-status-codes": "^2.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "sequelize-cli": "^6.5.2"
  },
  "_moduleAliases": {
    "#root": ".",
    "#app": "src",
    "#models": "models"
  },
  "repository": "git@github.com:kotstok/express-skeleton.git"
}
