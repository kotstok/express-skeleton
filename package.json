{
  "name": "express-test",
  "version": "1.0.0",
  "main": "src/app.js",
  "extension": "js",
  "license": "MIT",
  "type": "commonjs",
  "author": "github.com/kotstok",
  "private": true,
  "scripts": {
    "prestart": "babel ./src --out-dir dist",
    "start": "prestart && node ./src/app.js",
    "start:dev": "nodemon ./src/app.ts",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**'",
    "postpretty": "yarn lint --fix",
    "db:dev:up": "docker compose --env-file ./.env up dev-db -d",
    "db:dev:rm": "docker compose rm dev-db -s -f -v && docker container rm dev-db -f -v",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up",
    "db:test:up": "docker compose --env-file ./.env.test up test-db -d",
    "db:test:rm": "docker compose rm test-db -s -f -v && docker container rm test-db -f -v",
    "db:test:restart": "db:test:rm && yarn db:test:up"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.7",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/sequelize": "^4.28.14",
    "@types/winston": "^2.4.4",
    "babel-register": "^6.26.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "sequelize-cli": "^6.5.2"
  }
}
